@model List<PortalCG.Models.JsonModels.Discipline>
@{
    ViewBag.Title = "Disciplinas";
    const int ITEM_PER_ROW = 3;
}
@if (Model?.Count > 0)
{
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-md-@(12/ITEM_PER_ROW)">
                @Html.Partial("_DisciplinePartial", item)
            </div>
        }
    </div>
}